
<%= simple_form_for(@reservation) do |f| %>
  <%= f.input :begin_date, placeholder: "mojito" %>
  <%= f.input :end_date, placeholder: "mojito" %>
  <%= f.input :artist_id, placeholder: "mojito" %>
  <%= f.input :user_id, placeholder: "mojito" %>
  <%= f.input :status, placeholder: "mojito" %>
  <%= f.input :final_price, placeholder: "mojito" %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
